page { 
  background-color: #f5f5f5;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.search {
  width: 100%ï¼›;
}

.search_content {
  width: 94%;
  background: #fff;
  border-radius: 10rpx;
  margin: 20rpx auto;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}

.search_content icon {
  margin: 0 10rpx;
  line-height: 70rpx;
}

.search_content input {
  width: 96%;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0.2rem;
  border: 0;
  -webkit-box-sizing: content-box;
  box-sizing: content-box;
  background: transparent;
  color: #8f8f8f;
}

.search_content .shaixuan {
  display: inline-block;
  font-size: 28rpx;
  color: #1998fe;
  margin: 0 20rpx;
  white-space: nowrap;
}

.blindDate_container {
  padding: 20rpx;
}

.blindDate_container .blindDate_con {
  padding-bottom: 20rpx;
}

.blindDate_con .blindDate_panel {
  position: relative;
  width: 100%;
  height: 320rpx;
  background-repeat: no-repeat;
  background-position: center top;
  background-size: cover;
  color: #fff;
}

.blindDate_con .blindDate_panel .blindDate_tips {
  width: 180rpx;
  border-radius: 0.2rem;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 32rpx;
  padding: 10rpx 0;
}

.blindDate_con .blindDate_detail {
  background-color: #fff;
  padding: 10rpx;
  color: #666;
  line-height: 48rpx;
  position: relative;
}

.blindDate_detail .blindDate_title {
  font-size: 32rpx;
  font-weight: 500;
}

.blindDate_desc {
  font-size: 28rpx;
  color: #b3b3b3;
}

.blindDate_desc .blindDate_count {
  display: flex;
}

.blindDate_desc .blindDate_count .blindDate_gz,
.blindDate_desc .blindDate_count .blindDate_bm {
  display: inline-block;
  width: 30%;
}

.blindDate_desc .blindDate_count .blindDate_bm icon {
  margin-bottom: 10rpx;
}

.blindDate_desc .blindDate_date {
  display: flex;
}

.blindDate_desc .blindDate_date image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.blindDate_desc .blindDate_date text {
  line-height: 32rpx;
}

.blindDate_act {
  position: absolute;
  bottom: 10%;
  right: 5%;
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  border-radius: 50%;
  background-color: #f8a11b;
  text-align: center;
}

.blindDate_act text {
  font-size: 28rpx;
  color: #fff;
}

icon {
  margin-right: 20rpx;
  display: inline-flex;
}

/* active  */

.blindDate_detail.active .blindDate_title text:nth-of-type(1),
.blindDate_detail.active icon, .blindDate_count text {
  color: #ff2f17;
}

/* live */

.blindDate_detail.live .blindDate_title text:nth-of-type(1),
.blindDate_detail.live icon, .blindDate_count text {
  color: #f8a11b;
}

/* end */

.blindDate_detail.end .blindDate_title text:nth-of-type(1),
.blindDate_detail.end icon, .blindDate_count text {
  color: #b1b1b1;
}

/* active  */

.blindDate_detail.active .blindDate_act {
  background-color: #ff2f17;
}

/* live  */

.blindDate_detail.live .blindDate_act {
  background-color: #f8a11b;
}

/* end  */

.blindDate_detail.end .blindDate_act {
  background-color: #b1b1b1;
}

@keyframes myfirst {
  from {
    transform: translateY(-860rpx);
  }

  to {
    transform: translateY(0rpx);
  }
}

@keyframes mylast {
  from {
    transform: translateY(0rpx);
  }

  to {
    transform: translateY(-900rpx);
  }
}

.filter-panel {
  width: 90%;
  color: #908f92;
  padding: 25rpx 0;
  border-bottom: 1px solid #f2f2f2;
  margin: 0 auto;
}

.filter-panel:last-of-type {
  border-bottom: none;
}

.filter-panel .filter-title {
  line-height: 34rpx;
  font-size: 34rpx;
  font-weight: 500;
  margin-bottom: 30rpx;
}

.filter-panel .filter-content {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.filter-panel .filter-content:after {
  content: "";
  width: 46%;
}

.filter-content view {
  font-size: 30rpx;
  width: 23%;
  height: 60rpx;
  text-align: center;
  box-sizing: border-box;
}

.filter-content view.active {
  background-color: #fff;
  border-radius: 5px;
  border: 1px solid #70c466;
  color: #70c466;
}

.picker-city {
  display: inline-block;
  width: 50%;
}

.picker-city view {
  width: 100%;
}

.datingTime-panel {
  display: flex;
  width: 75% !important;
}

.picker-datingTime {
  display: inline-block;
  width: 100%;
}

.picker-datingTime view {
  width: 100%;
}

.test2 {
  animation: mylast;
  animation-duration: 0.5s;
  animation-iteration-count: 1;
  animation-fill-mode: forwards;
  width: 100%;
  height: 60%;
  border: 2rpx solid #ededed;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1454;
  background-color: #fff;
}

.test1 {
  animation: myfirst;
  animation-duration: 1s;
  animation-iteration-count: 1;
  animation-fill-mode: forwards;
  width: 100%;
  height: 60%;
  border: 2rpx solid #ededed;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1454;
  background-color: #fff;
}

.btn {
  width: 680rpx;
  height: 84rpx;
  line-height: 84rpx;
  margin: 10rpx auto;
  color: #70c466;
  background-color: #fff;
}
