.partner {
  display: flex;
  align-items: center;
  height: 120rpx;
  background-color: #fff;
  font-size: 32rpx;
  border-bottom: 1px solid #f2f2f2;
}

.partner image {
  margin-left: 30rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 5px;
}

.partner label:nth-of-type(2) {
  margin-right: 30rpx;
}

.partner  label:nth-of-type(1) {
  flex-grow: 1;
  margin-left: 30rpx;
}
