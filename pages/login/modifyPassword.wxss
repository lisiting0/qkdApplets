.m_mdp_list {
  padding: 0 30rpx 30rpx;
  background-color: #fff;
}

.m_mdp_list input+ input {
  border-top: 1px solid #f2f2f2;
}

.input-panel {
  align-items: center;
  display: flex;
  justify-content: space-between;
  border-bottom:1px solid #f2f2f2; 
}

.input-panel input {
  width: 90%;
}

.input-panel image {
  width: 40rpx;
  height: 30rpx;
}

.forget-text {
  float: right;
  margin-right: 30rpx;
  color: #f00;
  font-size: 32rpx;
}
