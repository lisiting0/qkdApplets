.jy_jw_top {
  position: relative;
  height: 240rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}

.jy_jw_top image {
  width: 100%;
  height: 100%;
}

.jy_jw_top_position {
  width: 94%;
  position: absolute;
  top: 5%;
  left: 6%;
}

.jy_jw_top_position .jy_jw_top_p {
  font-size: 32rpx;
  color: #fff;
  filter: alpha(opacity=60);
  opacity: 0.6;
  -moz-opacity: 0.6;
  padding: 40rpx 0;
}

.jy_jw_top_position .jy_jw_top_m {
  position: relative;
  width: 100%;
  color: #fff;
}

.jy_jw_top_m text:first-child {
  font-size: 68rpx;
}

.jy_jw_top_position .jy_jw_tx {
  font-size: 36rpx;
  color: #16a5d9;
  background-color: #fff;
  border-radius: 5px;
  padding: 5rpx 20rpx;
  position: absolute;
  right: 4%;
  top: 0;
}

.jy_jw_money {
  width: 100%;
  height: 110rpx;
  background-color: #fff;
  padding: 25rpx 0;
  clear: both;
}

.jy_jw_money .jy_jw_money_li {
  width: 33.33%;
  float: left;
  font-size: 28rpx;
  color: #c8c8c8;
  text-align: center;
  padding-bottom: 20rpx;
}

.jy_jw_money .jy_jw_money_li text:last-child {
  font-size: 40rpx;
  color: #515151;
}

.jy_jw .jy_jw_tit {
  color: #a7a7a7;
  font-size: 32rpx;
  padding: 20rpx;
}

.jy_jw_money_li image {
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx auto 6rpx;
}

.jy_jw_money_li .tg_text {
  font-size: 36rpx !important;
}

