.jy_block_t {
  margin-top: 20rpx;
  background-color: #fff;
  overflow: hidden;
}

.jy_p_tit2 {
  font-size: 36rpx;
  color: #000;
  font-weight: bold;
  padding: 25rpx;
}

.jy_p_tit2:before {
  content: "\e664";
  color: #ff0048;
  font-size: 32rpx;
  font-weight: normal;
  margin-right: 30rpx;
  vertical-align: top;
}

.jy_aInfo_line_li {
  position: relative;
  overflow: hidden;
  padding: 20rpx 25rpx;
  border-bottom: 1px solid #ebebeb;
  font-size: 32rpx;
  color: #151515;
}

.jy_aInfo_line_li::before {
  content: "\e702";
  position: absolute;
  right: 25rpx;
  font-size: 32rpx;
  margin-top: -20rpx;
  top: 50%;
  color: #c7c6cc;
  font-weight: bold;
}
