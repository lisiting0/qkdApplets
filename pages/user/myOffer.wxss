::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.weui-navbar {
  position: relative;
  border-bottom: none;
  background-color: #fff;
}

.weui-navbar__item {
  padding: 20rpx 0 0;
  color: #9a9a9d;
}

.weui-navbar__item.weui-bar__item_on {
  color: #000;
}

.weui-navbar__slider {
  width: 2em;
  background-color: #ff4300;
}

.jxz_list {
  clear: both;
}

.jxz_list .jxz_title {
  color: #4c4c4c;
  font-weight: 500;
  margin: 30rpx 0;
  border-left: 15rpx solid #ff4300;
  padding-left: 20rpx;
}

.myOffer_list_li {
  float: left;
  width: 33.3%;
  padding: 10rpx 0;
}

.processing {
  width: 200rpx;
  height: 180rpx;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
}

.playing {
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  border-radius: 50%;
  background-color: #ff4300;
  border: 1px solid #fff;
  text-align: center;
  position: absolute;
  right: 0.2rem;
  bottom: -0.2rem;
}

.playing text {
  display: inline-block;
  width: 1.5px;
  border-radius: 0.1rem;
  background-color: #fff;
  text-align: center;
  margin: 0 1px;
}

.playing text:nth-of-type(1) {
  height: 0.1rem;
  animation: playing 0.6s infinite 0.2s;
  -webkit-animation: playing 0.6s infinite 0.2s;
}

.playing text:nth-of-type(2) {
  height: 0.2rem;
  animation: playing 0.6s infinite 0.4s;
  -webkit-animation: playing 0.6s infinite 0.4s;
}

.playing text:nth-of-type(3) {
  height: 0.15rem;
  animation: playing 0.6s infinite 0.6s;
  -webkit-animation: playing 0.6s infinite 0.6s;
}

@keyframes playing {
  0% {
    height: 0;
  }

  20% {
    height: 30%;
  }

  50% {
    height: 60%;
  }

  80% {
    height: 30%;
  }

  100% {
    height: 0;
  }
}

.user-image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  position: absolute;
  bottom: -0.75rem;
  left: 0.2rem;
}

.aliasName {
  font-size: 32rpx;
  color: #242424;
  padding: 0rpx 0 0 90rpx;
}

.datingDesc {
  font-size: 28rpx;
  color: #666;
  padding: 0.2rem 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.countdown {
  width: auto;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  border-radius: 10rpx;
  padding: 0 10rpx;
  position: absolute;
  left: 23%;
  top: 35%;
  background: -webkit-linear-gradient(left, #ffb533, #ffb533, #ff7900); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #ffd455, #ffb533, #ff7900); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #ffd455, #ffb533, #ff7900); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #ffd455, #ffb533, #ff7900); /* 标准的语法（必须放在最后） */
}

.countdown  .countdown_icon {
  display: inline-block;
  width: 36rpx;
  height: 40rpx;
}

.time {
  width: auto;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  border-radius: 10rpx;
  padding: 0 10rpx;
  position: absolute;
  left: 10%;
  top: 35%;
  background: -webkit-linear-gradient(left, #d59730, #d46f05, #d46901); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #d59730, #d46f05, #d46901); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #d59730, #d46f05, #d46901); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #d59730, #d46f05, #d46901); /* 标准的语法（必须放在最后） */
}

