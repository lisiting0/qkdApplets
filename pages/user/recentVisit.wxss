page{
  overflow: hidden;
}

::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}

.weui-navbar{
  position: relative; 
  border-bottom: none;
  background-color: #fff;
}

.weui-navbar__item{
  padding: 20rpx 0 0;
  color: #9a9a9d;
}

.weui-navbar__item.weui-bar__item_on{
  color: #000;
}

.weui-navbar__slider{
  width: 2em;
  background-color: #ff4300;
}

.weui-search-bar{
  width: 100%; 
}

.searchbar-result{ 
    margin-top: 0;
    font-size: 14px;
}

.searchbar-result:before{
    display: none;
}

.weui-cell{
    padding: 12px 15px 12px 35px;
} 