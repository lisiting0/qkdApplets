/* 约会类型 */

.jy-type {
  font-size: 28rpx;
  font-weight: 300;
  text-align: center;
  border-radius: 3px;
  color: #fff;
  margin-right: 10rpx;
  margin-left: 10rpx;
  padding: 10rpx 15rpx;
}

.jy-type.xy {
  background: -webkit-linear-gradient(left, #16c8de, #3ad0ea, #5ed7f7); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #16c8de, #3ad0ea, #5ed7f7); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #16c8de, #3ad0ea, #5ed7f7); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #16c8de, #3ad0ea, #5ed7f7); /* 标准的语法（必须放在最后） */
}

.jy-type.qy {
  background: -webkit-linear-gradient(left, #f785ff, #f364fc, #f049fa); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #f785ff, #f364fc, #f049fa); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #f785ff, #f364fc, #f049fa); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #f785ff, #f364fc, #f049fa); /* 标准的语法（必须放在最后） */
}

.jy-type.jy {
  background: -webkit-linear-gradient(left, #ffb533, #ffb533, #ff7900); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #ffd455, #ffb533, #ff7900); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #ffd455, #ffb533, #ff7900); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #ffd455, #ffb533, #ff7900); /* 标准的语法（必须放在最后） */
}

.jy-type.zy {
  background: -webkit-linear-gradient(left, #4b7dfa, #6f78fa, #9074fa); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #4b7dfa, #6f78fa, #9074fa); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #4b7dfa, #6f78fa, #9074fa); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #4b7dfa, #6f78fa, #9074fa); /* 标准的语法（必须放在最后） */
}

.jy-type.py {
  background: -webkit-linear-gradient(left, #35df32, #24c532, #15af31); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #35df32, #24c532, #15af31); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #35df32, #24c532, #15af31); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #35df32, #24c532, #15af31); /* 标准的语法（必须放在最后） */
}

.jy-type.xmdx {
  background: -webkit-linear-gradient(left, #e55fa4, #e55fa4, #e366a2); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(left, #e55fa4, #e55fa4, #e366a2); /* Opera 11.6 - 12.0 */
  background: -moz-linear-gradient(left, #e55fa4, #e55fa4, #e366a2); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #e55fa4, #e55fa4, #e366a2); /* 标准的语法（必须放在最后） */
}

